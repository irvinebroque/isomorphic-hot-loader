{
  "name": "isomorphic-hot-loader",
  "version": "0.0.4",
  "description": "An actively maintained starter project for building isomorphic web apps with React.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/irvinebroque/isomorphic-hot-loader.git"
  },
  "main": "server.js",
  "author": "Brendan Irvine-Broque <brendanib@gmail.com>",
  "scripts": {
    "dev": "NODE_ENV=development supervisor --harmony --ignore build/ -e js server & NODE_ENV=development node hotLoadServer.js",
    "start": "NODE_ENV=production node --harmony server",
    "lint": "eslint ./",
    "typecheck": "flow",
    "build": "webpack -p --progress --colors",
    "test": "jest && eslint ./"
  },
  "eslintConfig": {
    "ecmaFeatures": {
      "jsx": true,
      "blockBindings": true,
      "forOf": true,
      "generators": true
    },
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "strict": [
        2,
        "global"
      ],
      "no-underscore-dangle": false,
      "quotes": false,
      "no-unused-vars": false,
      "new-cap": false
    }
  },
  "jest": {
    "scriptPreprocessor": "preprocessor.js",
    "unmockedModulePathPatterns": [
      "node_modules/react"
    ]
  },
  "dependencies": {
    "6to5": "^3.3.4",
    "6to5-runtime": "^3.3.4",
    "body-parser": "^1.6.4",
    "chroma-js": "^0.6.3",
    "compression": "^1.3.0",
    "cors": "^2.5.2",
    "express": "^4.3.2",
    "flux": "^2.0.1",
    "fuzzy": "^0.1.0",
    "isomorphic-fetch": "^1.7.0",
    "moment": "^2.8.4",
    "node-jsx": "^0.12.0",
    "object-assign": "^2.0.0",
    "raf": "^2.0.4",
    "react": "^0.12.0",
    "react-document-title": "^0.1.3",
    "react-router": "^0.11.6",
    "react-scroll-components": "^0.1.0",
    "react-tappable": "^0.2.5",
    "react-tween-state": "0.0.4",
    "rest": "^1.2.0",
    "ua-parser-js": "^0.7.3",
    "when": "^3.6.3"
  },
  "devDependencies": {
    "6to5-core": "^3.1.1",
    "6to5-loader": "^3.0.0",
    "eslint": "^0.13.0",
    "jest-cli": "^0.2.1",
    "json-loader": "^0.5.1",
    "jsx-loader": "^0.12.0",
    "react-hot-loader": "^1.0.4",
    "react-tools": "^0.12.2",
    "supervisor": "^0.6.0",
    "webpack": "^1.5.3",
    "webpack-dev-server": "^1.7.0"
  }
}
